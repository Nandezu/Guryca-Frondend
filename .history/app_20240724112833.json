{
  "expo": {
    "name": "Nandefrond",
    "slug": "Nandefrond",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nandefrond",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/nandezus.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.nandefrond",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Nandefrond to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Nandefrond to save photos.",
        "NSCameraUsageDescription": "Allow Nandefrond to use your camera."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.nandefrond",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Nandefrond to access your photos.",
          "savePhotosPermission": "Allow Nandefrond to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Nandefrond to access your photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}